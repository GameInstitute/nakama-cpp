{
    "name": "nakama-sdk",
    "version-string": "2.6.0",
    "default-features": [
        "wslay",
        "curl"
    ],
    "dependencies": [
        "protobuf",
        "rapidjson",
        "optional-lite"
    ],
    "features": {
        "wslay": {
            "name": "wslay",
            "platform": "linux | osx | ios"
        },
        "curl": {
            "name": "curl",
            "platform": "linux | osx | ios",
            "default-features": false,
            "features": [
                "ssl"
            ]
        },
        "libcxx": {
            "description": "Build using libc++",
            "dependencies": [
                {
                    "name": "llvm",
                    "platform": "linux",
                    "default-features": false,
                    "features": [
                        "libcxx"
                    ]
                }
            ]
        },
        "grpc": {
            "description": "Build GRPC client",
            "dependencies": [
                "grpc"
            ]
        }
    }
}